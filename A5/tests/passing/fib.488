{
    function fib(n : Integer) : Integer
    {
        if n <= 1 then return with n
        return with fib(n - 1) + fib(n - 2)
    }


    write "Should be 0: ", fib(0), newline
    write "Should be 1: ", fib(1), newline
    write "Should be 1: ", fib(2), newline
    write "Should be 2: ", fib(3), newline
    write "Should be 3: ", fib(4), newline
    write "Should be 5: ", fib(5), newline
    write "Should be 8: ", fib(6), newline
}
